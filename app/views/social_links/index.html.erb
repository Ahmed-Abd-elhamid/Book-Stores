<p id="notice"><%= notice %></p>

<h1 id="hd">Social Links</h1>

<table id="tbl">
  <thead>
    <tr>
      <th>Site</th>
      <th>Link</th>
      <th>Store</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @social_links.each do |social_link| %>
      <tr>
        <td><%= social_link.site %></td>
        <td><%= social_link.link %></td>
        <td><%= social_link.store_id %></td>
        <td><%= link_to 'Show', social_link %></td>
        <td><%= link_to 'Edit', edit_social_link_path(social_link) %></td>
        <td><%= link_to 'Destroy', social_link, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table id="tbl">

<br>

<%= link_to 'New Social Link', new_social_link_path %>
