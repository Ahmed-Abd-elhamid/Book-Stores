<p id="notice"><%= notice %></p>

<h1 id="hd">Rates</h1>

<table id="tbl">
  <thead>
    <tr>
      <th>Rate</th>
      <th>User</th>
      <th>Book</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rates.each do |rate| %>
      <tr>
        <td><%= rate.rate %></td>
        <td><%= rate.user_id %></td>
        <td><%= rate.book_id %></td>
        <td><%= link_to 'Show', rate %></td>
        <td><%= link_to 'Edit', edit_rate_path(rate) %></td>
        <td><%= link_to 'Destroy', rate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table id="tbl">

<br>

<%= link_to 'New Rate', new_rate_path %>
